route_pattern.display_order display_order, location.location_id, location.location_code, location.location_name description, SUM(people_count_fact.in_count) total_in, SUM(people_count_fact.out_count) total_out, AVG(people_count_fact.in_count) avg_in, AVG(people_count_fact.out_count) avg_out, 
time_dimension.hour_no arrival_hour 
FROM report_journeys, timetable_journey_fact,  route_dimension route, route_pattern, location_dimension location, vehicle_dimension vehicle, time_dimension, timetable_visit_fact left join people_count_fact ON timetable_visit_fact.fact_id = people_count_fact.visit_fact_id
WHERE 1 = 1  
AND report_journeys.journey_id = timetable_journey_fact.fact_id
AND timetable_journey_fact.fact_id = timetable_visit_fact.journey_fact_id
AND timetable_visit_fact.arrival_time IS NOT NULL
AND route.route_id = timetable_journey_fact.route_id
AND route_pattern.route_id = timetable_journey_fact.route_id
AND route_pattern.location_id = timetable_visit_fact.location_id
AND location.location_id = timetable_visit_fact.location_id
AND timetable_visit_fact.arrival_time_id = time_dimension.time_id
AND vehicle.vehicle_id = timetable_journey_fact.vehicle_id GROUP BY route.route_code, route_pattern.display_order, location.location_id, location.location_code, location.location_name, 10 
ORDER BY  route.route_code ASC, arrival_hour ASC, route_pattern.display_order ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>route_code</Name><TableName>route</TableName><ColumnName>route_code</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>display_order</Name><TableName>route_pattern</TableName><ColumnName>display_order</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>location_id</Name><TableName>location</TableName><ColumnName>location_id</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>l