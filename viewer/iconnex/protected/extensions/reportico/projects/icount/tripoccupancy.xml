date_format(timetable_journey_fact.actual_start, '%d-%m-%Y' )
 count_date
FROM report_journeys JOIN timetable_journey_fact ON report_journeys.journey_id = timetable_journey_fact.fact_id  
JOIN timetable_journey ON timetable_journey.timetable_id = timetable_journey_fact.timetable_id
JOIN timetable_visit_fact ON timetable_journey_fact.fact_id =  timetable_visit_fact.journey_fact_id
JOIN route_visibility route ON route.route_id = timetable_journey.route_id
