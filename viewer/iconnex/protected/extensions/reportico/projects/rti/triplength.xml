<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Trip Length Report</ReportTitle><ReportDescription>The &lt;b&gt;Trip Length&lt;/b&gt; Report identifies the actual length each tracked trip has taken from the actual departure time at the first location to the arrival time at the final stop for the journeys that match the entered criteria</ReportDescription><PageSize>A4</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont>Helvetica</pdfFont><pdfFontSize>10.0</pdfFontSize><PreExecuteCode>$dfrom = $_criteria[&quot;date&quot;]-&gt;get_criteria_value(&quot;RANGE1&quot;);
$dto = $_criteria[&quot;date&quot;]-&gt;get_criteria_value(&quot;RANGE2&quot;);

$dfdy = substr($dfrom, 1,2);
$dfmn = substr($dfrom, 4,2);
$dfyr = substr($dfrom, 7,4);
$dtdy = substr($dto, 1,2);
$dtmn = substr($dto, 4,2);
$dtyr = substr($dto, 7,4);

$ifrom = mktime ( 0, 0, 0, $dfmn, $dfdy, $dfyr );
$ito = mktime ( 0, 0, 0, $dtmn, $dtdy, $dtyr );



//echo &quot;&lt;BR&gt;$dfrom Fr  d$dfdy m$dfmn y$dfyr &lt;BR&gt;&quot;;
//echo &quot;&lt;BR&gt;$dto To  $dtdy $dtmn $dtyr &lt;BR&gt;&quot;;

$sql = &quot;CREATE TEMP TABLE t_days ( day date );&quot;;
//echo $sql.&quot;&lt;BR&gt;&quot;;

$ds-&gt;Execute($sql) or print $ds-&gt;ErrorMsg();

$ptr = $ifrom;
while ( $ptr &lt;= $ito )
{
    $dt = strftime ( &quot;%d/%m/%Y&quot;, $ptr );

    $sql = &quot;INSERT INTO t_days VALUES ( '&quot;.$dt.&quot;' );&quot;;
//echo $sql.&quot;&lt;BR&gt;&quot;;
    $ds-&gt;Execute($sql) or print $ds-&gt;ErrorMsg();

    $ptr = $ptr + ( 24 * 60 * 60 );
}



//echo &quot;&lt;BR&gt;&lt;BR&gt;From...$dfdy, $dfmn, $dfyr&lt;BR&gt;&lt;BR&gt;&quot;;


$sql = &quot;SELECT t_days.day day, service.route_id, service.service_id, max(rpat_orderby) trip_total
FROM operator,route_visibility route,service,service_patt, t_days
WHERE 1 = 1
AND operator.operator_id = route.operator_id
AND route.route_id = service.route_id
AND service_patt.service_id = service.service_id
AND usernm = USER
and t_days.day between wef_date and wet_date&quot;;

$rtval = $this-&gt;lookup_queries[&quot;route&quot;]-&gt;get_criteria_clause(false, true );
$opval = $this-&gt;lookup_queries[&quot;operator&quot;]-&gt;get_criteria_clause(false, true );


if ( $rtval )
    $sql .= &quot; and route.route_id &quot;. $rtval. &quot; &quot;;
if ( $opval )
    $sql .= &quot; and operator.operator_id &quot;. $opval. &quot; &quot;;


$sql .= &quot; group by 1, 2, 3 into temp t_summary; &quot;;
//echo $sql.&quot;&lt;BR&gt;&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();


$sql = &quot;SELECT archive_rt.schedule_id, arrival_time, departure_time_pub arrival_time_pub
FROM archive_rt, archive_rt_loc, t_summary, publish_tt
WHERE 1 = 1
AND t_summary.service_id = publish_tt.service_id
AND archive_rt.pub_ttb_id = publish_tt.pub_ttb_id
AND date(actual_start) = t_summary.day
AND archive_rt.schedule_id = archive_rt_loc.schedule_id
AND arrival_status = 'A'
AND rpat_orderby = t_summary.trip_total
INTO TEMP t_end_times WITH NO LOG
&quot;;

//echo $sql.&quot;&lt;BR&gt;&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();


$sql = &quot;SELECT archive_rt.schedule_id, departure_time, departure_time_pub
FROM archive_rt, archive_rt_loc, t_summary, publish_tt
WHERE 1 = 1
AND t_summary.service_id = publish_tt.service_id
AND archive_rt.pub_ttb_id = publish_tt.pub_ttb_id
AND date(actual_start) = t_summary.day
AND archive_rt.schedule_id = archive_rt_loc.schedule_id
AND rpat_orderby = 1
AND departure_status = 'A'
INTO TEMP t_start_times WITH NO LOG
&quot;;

//echo $sql.&quot;&lt;BR&gt;&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();

