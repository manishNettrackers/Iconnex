FROM operator, route_visibility route, timetable_journey_live active_rt,timetable_visit_live active_rt_loc, timetable_journey tj, employee,vehicle,location, outer ( active_rt_duty, employee emp2 ), outer employee emp3, outer pt_duty 
WHERE 1 = 1                      
AND usernm = USER
AND tj.route_id = route.route_id
AND operator.operator_id = route.operator_id
AND active_rt.driver_id = employee.employee_id
AND active_rt.vehicle_id = vehicle.vehicle_id
AND active_rt_loc.journey_fact_id = active_rt.fact_id
AND active_rt.timetable_id = tj.timetable_id
AND active_rt_loc.location_id = location.location_id
AND active_rt_duty.schedule_id = active_rt_loc.journey_fact_id
AND active_rt_duty.rpat_orderby = active_rt_loc.sequence
AND active_rt_duty.employee_id = emp2.employee_id  
AND active_rt.driver2_id = emp3.employee_id  
AND tj.ext_timetable_id = pt_duty.pub_ttb_id
AND active_rt_loc.sequence = pt_duty.rpat_orderby  
AND pt_duty.rpat_orderby &gt; 1  
