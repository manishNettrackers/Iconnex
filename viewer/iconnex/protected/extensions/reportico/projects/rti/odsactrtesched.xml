active_rt_loc.sequence rpat_orderby, 
active_rt_loc.arrival_time arrival_time, 
active_rt_loc.departure_time departure_time, active_rt_loc.departure_time_pub departure_time_pub, 
active_rt.fact_id schedule_id, tj.start_time start_time, active_rt_loc.departure_status actual_est, active_rt_loc.departure_status departure_status, active_rt_loc.arrival_status arrival_status, 
