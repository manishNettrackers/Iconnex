FROM user_route, cent_user, route
WHERE user_route.userid = cent_user.userid
AND cent_user.usernm = USER
AND route.operator_id = user_route.operator_id
UNION
SELECT route.*
FROM user_route, cent_user, route
WHERE user_route.userid = cent_user.userid
AND cent_user.usernm = USER
AND route.route_id = user_route.route_id
UNION
SELECT route.*
FROM route, cent_user
WHERE 1 = 1 AND 1 = 1
AND (
route.operator_id = cent_user.operator_id
OR cent_user.operator_id IS NULL )
AND cent_user.usernm = USER
