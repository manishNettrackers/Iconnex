<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Location Compliance Table</ReportTitle><ReportDescription>The &lt;b&gt;Compliance Analysis&lt;/b&gt; report shows for each location on a route how compliant the departure times of vehicles are to the timetable. The scheduled number of arrivals, the total number of departures from the location are shown along with how many were compliant.
&lt;p&gt;&lt;hr&gt;&lt;p&gt;
The compliance thresholds control the timeband used to measure compliance.
&lt;p&gt;&lt;hr&gt;&lt;p&gt;
The lateness tolerance indicates a threshold outside which trips are deemed unsuitable for compliance reporting and are therefore excluded from the report.
&lt;p&gt;&lt;hr&gt;&lt;p&gt;
Select the combination of Route, Date Range, Location, Days of the Week, and Thresholds you require or Accept the default values. 
&lt;P&gt;&lt;HR&gt;&lt;P&gt;
Select the type of Output and Level of Detail required and &lt;B&gt;Execute&lt;/B&gt;</ReportDescription><PageSize>A4</PageSize><PageOrientation>Portrait</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>$dfrom = $_criteria[&quot;date&quot;]-&gt;get_criteria_value(&quot;RANGE1&quot;);
$dto = $_criteria[&quot;date&quot;]-&gt;get_criteria_value(&quot;RANGE2&quot;);
$rt = $_criteria[&quot;route&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$op = $_criteria[&quot;operator&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$loc = $_criteria[&quot;location&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$wkd = $_criteria[&quot;weekday&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$grp = $_criteria[&quot;grouping&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$lowlat = $_criteria[&quot;lowerlateness&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$uplat = $_criteria[&quot;upperlateness&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$latetol = $_criteria[&quot;latetol&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);
$starts = $_criteria[&quot;startpoint&quot;]-&gt;get_criteria_value(&quot;VALUE&quot;);

$gloc = false;
$grte = false;
$gday = false;
if ( preg_match(&quot;/L/&quot;, $grp )) $gloc = true;
if ( preg_match(&quot;/R/&quot;, $grp )) $grte = true;
if ( preg_match(&quot;/D/&quot;, $grp )) $gday = true;


$dfdy = substr($dfrom, 1,2);
$dfmn = substr($dfrom, 4,2);
$dfyr = substr($dfrom, 7,4);
$dtdy = substr($dto, 1,2);
$dtmn = substr($dto, 4,2);
$dtyr = substr($dto, 7,4);

$ifrom = mktime ( 0, 0, 0, $dfmn, $dfdy, $dfyr );
$ito = mktime ( 0, 0, 0, $dtmn, $dtdy, $dtyr );

$sql = &quot;SET ISOLATION TO DIRTY READ&quot;;
$ds-&gt;Execute($sql) or print $ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
SELECT route.*
FROM route, cent_user 
WHERE 1 = 1 
AND (
route.operator_id = cent_user.operator_id
OR cent_user.operator_id IS NULL )
AND cent_user.usernm = USER
INTO TEMP t_route WITH NO LOG
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql = &quot;CREATE TEMP TABLE t_days ( day date, dtime datetime year to day ) with no log;&quot;;
$ds-&gt;Execute($sql) or print $ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$ptr = $ifrom;
$dtdesc = &quot;&quot;;
while ( $ptr &lt;= $ito )
{
    $dt = strftime ( &quot;%d/%m/%Y&quot;, $ptr );
    $dtm = strftime ( &quot;%Y-%m-%d&quot;, $ptr );

    if ( $ptr == $ifrom ) $dtdesc = &quot;$dt - &quot;;
    if ( $ptr == $ito ) $dtdesc .= &quot;$dt&quot;;
    if ( $ptr == $ito &amp;&amp; $ptr == $ifrom ) $dtdesc = &quot;$dt&quot;;
    $sql = &quot;INSERT INTO t_days VALUES ( '&quot;.$dt.&quot;', '&quot;.$dtm.&quot;' );&quot;;
    $ds-&gt;Execute($sql) or print $ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

    $ptr = $ptr + ( 24 * 60 * 60 );
};


$sql =&quot;
DELETE FROM t_days WHERE WEEKDAY(day) NOT IN ( $wkd );
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;



$sql = &quot;
SELECT t_days.day, t_days.dtime, route_code, service_code,
t_route.route_id, operator.operator_code operator_code,
service.service_id, publish_tt.pub_ttb_id pub_ttb_id, notes[1,1] over_midnight, publish_tt.pub_prof_id
FROM operator,t_route,service, t_days, publish_tt, event, event_pattern
WHERE 1 = 1
AND operator.operator_id = t_route.operator_id
AND t_route.route_id = service.route_id
AND publish_tt.service_id = service.service_id
and t_days.day between service.wef_date and service.wet_date
AND publish_tt.evprf_id = event_pattern.evprf_id
AND event_pattern.event_id = event.event_id
AND event.event_tp = 3
and weekday(t_days.day) between rpdy_start and rpdy_end
and ( current &gt; extend(start_time, year to second) or
date(current) &gt; t_days.day)
&quot;;
if ( $rt )
    $sql .= &quot; AND t_route.route_id in ( $rt )&quot;;
if ( $op )
    $sql .= &quot; AND operator.operator_id in ( $op )&quot;;

$sql .=&quot;
INTO TEMP t_timetable WITH NO LOG
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql=&quot;
SELECT service_id, max(rpat_orderby) rpat_orderby
FROM service_patt
WHERE service_id IN (
SELECT service_id FROM t_timetable
)
GROUP BY 1
INTO TEMP t_lastlocs WITH NO LOG;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
CREATE INDEX ix_tttb ON t_timetable ( day, pub_ttb_id );
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
CREATE INDEX ix_tttb2 ON t_timetable ( pub_ttb_id );
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql=&quot;
SELECT day, t_timetable.pub_ttb_id, t_timetable.route_id, route_location.location_id, 
  route_location.rpat_orderby
FROM t_timetable, route_location, publish_tt, service_patt, t_lastlocs
WHERE t_timetable.pub_prof_id = route_location.profile_id
AND t_timetable.pub_ttb_id = publish_tt.pub_ttb_id
AND publish_tt.service_id = service_patt.service_id
AND route_location.rpat_orderby = service_patt.rpat_orderby
AND service_patt.service_id = t_lastlocs.service_id
AND service_patt.rpat_orderby &lt; t_lastlocs.rpat_orderby
&quot;;
if ($starts)
   $sql .= &quot;AND route_location.rpat_orderby = 1&quot;;
if ($loc)
   $sql .= &quot;AND route_location.location_id IN ( $loc )&quot;;
$sql .= &quot;
INTO TEMP t_pubtime WITH NO LOG;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

if ( $gloc )
    $loc_grpid = &quot;0 location_id&quot;;
else
    $loc_grpid = &quot;location_id&quot;;

if ( $grte )
    $rte_grpid = &quot;0 route_id&quot;;
else
    $rte_grpid = &quot;route_id&quot;;

if ( $gday )
    $day_grpid = &quot;mdy(1,1,1899) day&quot;;
else
    $day_grpid = &quot;day&quot;;


$sql=&quot;
SELECT $day_grpid, $loc_grpid, $rte_grpid, count(*) sched_ct 
FROM t_pubtime
GROUP BY 1, 2,3
INTO TEMP t_pubcount WITH NO LOG;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
SELECT t_timetable.day, t_timetable.route_id,
             t_timetable.day r_day,
             t_timetable.route_id r_route_id,
             date(actual_start) start_date, t_timetable.pub_ttb_id pub_ttb_id,
             actual_start,
             archive_rt.schedule_id, t_pubtime.rpat_orderby
FROM t_timetable,archive_rt, t_pubtime
WHERE 1 = 1
and t_timetable.pub_ttb_id = archive_rt.pub_ttb_id
and t_timetable.day = date(actual_start)
and t_timetable.day = t_pubtime.day
and t_timetable.pub_ttb_id = t_pubtime.pub_ttb_id
into temp t_summary_ar with no log ;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


if ( $gloc )
    $loc_grpid = &quot;0 location_id&quot;;
else
    $loc_grpid = &quot;archive_rt_loc.location_id&quot;;

if ( $grte )
    $rte_grpid = &quot;0 route_id&quot;;
else
    $rte_grpid = &quot;t_summary_ar.route_id&quot;;


if ( $gday )
    $day_grpid = &quot;mdy(1,1,1899) day&quot;;
else
    $day_grpid = &quot;t_summary_ar.day&quot;;

$grpfld = &quot;&quot;;
$grpfld = &quot;'X'&quot;;

if ( !$gloc )
   $grpfld .= &quot;||'=L'||b.description&quot;;

if ( !$grte )
   $grpfld .= &quot;||'=R'||route_code&quot;;

if ( !$gday )
  $grpfld .= &quot;||'=D'&quot;;

$grpfld .= &quot; ordtext&quot;;

$sql=&quot;
SELECT 
&quot;.$day_grpid.&quot;,
&quot;.$rte_grpid.&quot;,
&quot;.$loc_grpid.&quot;,
archive_rt_loc.location_id r_location_id,
t_summary_ar.day r_day,
t_summary_ar.route_id r_route_id,
date(actual_start) start_date,
t_summary_ar.pub_ttb_id pub_ttb_id,
archive_rt_loc.actual_est actual_est,
archive_rt_loc.departure_status,
extend(archive_rt_loc.departure_time, hour to second) departure_time,
extend(archive_rt_loc.departure_time_pub, hour to second) departure_time_pub,
extend(departure_time, hour to second) -
extend(departure_time_pub, hour to second) lateness
FROM t_summary_ar, archive_rt_loc
WHERE 1 = 1
and t_summary_ar.schedule_id = archive_rt_loc.schedule_id
and t_summary_ar.rpat_orderby = archive_rt_loc.rpat_orderby
&quot;;
if ($loc)
$sql .= &quot;AND archive_rt_loc.location_id IN ( $loc )&quot;;
$sql .= &quot;
and archive_rt_loc.departure_time_pub IS NOT NULL
and departure_status = 'A'
and (extend(departure_time, hour to second) -
extend(departure_time_pub, hour to second) &lt; $latetol and
extend(departure_time_pub, hour to second) -
extend(departure_time, hour to second) &lt; $latetol)
into temp t_summary with no log
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
select $grpfld, t_summary.route_id, t_summary.location_id, day start_date, count(*) everything
from t_summary, route a, location b
where 1 = 1
and t_summary.r_route_id = a.route_id
and t_summary.r_location_id = b.location_id
group by 1, 2, 3, 4
into temp t_summary_all with no log;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql=&quot;
select $grpfld, t_summary.route_id, t_summary.location_id, day start_date, count(*) compliant
from t_summary, route a, location b
where 1 = 1
and t_summary.r_route_id = a.route_id
and t_summary.r_location_id = b.location_id
and lateness between $lowlat and $uplat
group by 1, 2, 3, 4
into temp t_summary_ontime with no log;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
select $grpfld, t_summary.route_id, t_summary.location_id, day start_date, count(*) early
from t_summary, route a, location b
where 1 = 1
and t_summary.r_route_id = a.route_id
and t_summary.r_location_id = b.location_id
and lateness &lt; $lowlat
group by 1, 2, 3, 4
into temp t_summary_early with no log;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql=&quot;
select $grpfld, t_summary.route_id, t_summary.location_id, day start_date, count(*) late
from t_summary, route a, location b
where 1 = 1
and t_summary.r_route_id = a.route_id
and t_summary.r_location_id = b.location_id
and lateness &gt; $uplat
group by 1, 2, 3, 4
into temp t_summary_late with no log;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql = &quot;
select '$dtdesc' dtdesc, t_summary_all.ordtext, t_summary_all.route_id, t_summary_all.location_id, t_summary_all.start_date, everything, early, compliant, late
from t_summary_all, outer t_summary_ontime, outer t_summary_early, outer t_summary_late
where t_summary_all.route_id = t_summary_ontime.route_id
and t_summary_all.location_id = t_summary_ontime.location_id
and t_summary_all.start_date = t_summary_ontime.start_date
and t_summary_all.route_id = t_summary_early.route_id
and t_summary_all.location_id = t_summary_early.location_id
and t_summary_all.start_date = t_summary_early.start_date
and t_summary_all.route_id = t_summary_late.route_id
and t_summary_all.location_id = t_summary_late.location_id
and t_summary_all.start_date = t_summary_late.start_date
INTO TEMP t_summary1 WITH NO LOG
;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

$sql = &quot;UPDATE t_summary1 SET early = 0 where early is null&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;
$sql = &quot;UPDATE t_summary1 SET late= 0 where late is null&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;
$sql = &quot;UPDATE t_summary1 SET compliant = 0 where compliant is null&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;



$sql=&quot;
select route_id, location_id, day start_date, count(*) observations
from t_summary
group by 1, 2, 3
into temp t_summary2 with no log;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql=&quot;
CREATE TEMP TABLE t_replocs(
  location_id INTEGER,
  location_code CHAR(12),
  description CHAR(30)
)WITH NO LOG;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


$sql=&quot;
CREATE TEMP TABLE t_reprtes(
  route_id INTEGER,
  route_code CHAR(12),
  description CHAR(30)
)WITH NO LOG;
&quot;;
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;

if ( $gloc )
{
$sql=&quot;
INSERT INTO t_replocs
VALUES ( 0, '*', '*' );
&quot;;
}
else
{
$sql=&quot;
INSERT INTO t_replocs
SELECT UNIQUE a.location_id, location_code, description
FROM location a, t_summary1 b
WHERE a.location_id = b.location_id
&quot;;
}
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


if ( $grte )
{
$sql=&quot;
INSERT INTO t_reprtes
VALUES ( 0, '*', '*' );
&quot;;
}
else
{
$sql=&quot;
INSERT INTO t_reprtes
SELECT UNIQUE a.route_id, route_code, description
FROM route a, t_summary1 b
WHERE a.route_id = b.route_id
&quot;;
}
$ds-&gt;Execute($sql) or print $sql.&quot;&lt;br&gt;&quot;.$ds-&gt;ErrorMsg();
//echo $sql.&quot;&lt;BR&gt;&lt;BR&gt;&quot;;


</PreExecuteCode><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType></DatabaseType><DatabaseName>centurion</DatabaseName><HostName>centlive_tcp</HostName><ServiceName>?Unknown?</ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql>t_summary1,t_summary2, t_reprtes, t_replocs, outer t_pubcount</TableSql><WhereSql>                     
