and t_summary1.location_id= t_summary2.location_id
and t_summary1.location_id = t_replocs.location_id
and t_summary1.start_date = t_summary2.start_date
and t_summary1.location_id = t_pubcount.location_id
and t_summary1.start_date = t_pubcount.day
and t_summary1.route_id = t_pubcount.route_id
